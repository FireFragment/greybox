{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "ext-intl": "*",
        "ext-gd": "*",
        "laravel/lumen-framework": "^9.0",
        "illuminate/contracts": "^9.36.3",
        "doctrine/dbal": "^3.2",
        "fakturoid/fakturoid-php": "1.0.3",
        "rikudou/czqrpayment": "^5.3",
        "endroid/qr-code": "^4.4.1",
        "bugsnag/bugsnag-laravel": "^2.21.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "illuminate/mail": "^9.52"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "1.3.3",
        "fakerphp/faker": "^1.19"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
