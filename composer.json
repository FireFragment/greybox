{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.5",
        "ext-intl": "*",
        "ext-gd": "*",
        "laravel/lumen-framework": "7.2.2",
        "vlucas/phpdotenv": "4.1.8",
        "illuminate/mail": "7.30.1",
        "doctrine/dbal": "2.10.4",
        "fakturoid/fakturoid-php": "1.0.3",
        "rikudou/czqrpayment": "4.0.2",
        "endroid/qr-code": "3.9.6",
        "loudislav/debatematch": "dev-master",
        "bugsnag/bugsnag-laravel": "2.21.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "1.9.2",
        "phpunit/phpunit": "8.5.13",
        "mockery/mockery": "1.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
