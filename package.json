{
  "name": "greybox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open --mode development",
    "serve:debug": "vue-cli-service serve --open --mode debug",
    "serve:prod": "vue-cli-service serve --open --mode production",
    "build": "vue-cli-service build --mode production",
    "build:debug": "vue-cli-service build --mode debug",
    "build:dev": "vue-cli-service build --mode development",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run dev"
  },
  "dependencies": {
    "@d0whc3r/vue-auth-plugin": "^1.2.0",
    "@quasar/extras": "^1.0.0",
    "@smartform.cz/smartform": "0.0.1",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "quasar": "^1.0.0-beta.1",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-i18n": "^8.11.2",
    "vue-moment": "^4.0.0",
    "vue-router": "^3.1.3",
    "vue-slide-up-down": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-imports": "1.5.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-quasar": "^1.0.0-rc.3",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
